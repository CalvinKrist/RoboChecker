module random_robot

	x2 : [1..10] init 1; // robot x position
	y2 : [1..10] init 1; // robot y position
	dir : [0..9] init 0; // possible robot directions
	counter : [0..8] init 0; // if the robot is moving
	
	// Movement transitions when robot can move
	[] (dir=0 & moveA00 & collideA00 & counter=0) -> 1 : (counter'=counter+1) & (x2'=x2+1) & (y2'=y2);
	[] (dir=0 & moveA01 & collideA01 & counter=1) -> 1 : (counter'=counter+1) & (x2'=x2+1) & (y2'=y2);
	[] (dir=0 & moveA02 & collideA02 & counter=2) -> 1 : (counter'=counter+1) & (x2'=x2+1) & (y2'=y2);
